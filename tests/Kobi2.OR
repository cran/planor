#library("planor")
source("initOR")
#---------------------------------------------------------------------------
# EXAMPLES FROM THE PLANOR MANUAL
#---------------------------------------------------------------------------
# Exemple 2 page 12
#---------------------------------------------------------------------------
cat("\n")
cat("***************** EXEMPLE 2 PAGE 12 *****************\n")
cat("\n")
cat("Four treatment factors at 6, 6, 4, 2 levels and one 6-level block factor\n")
cat("Model: bloc+(A+B+C+D)^2\n")
cat("Estimate: A+B+C+D\n")
cat("N=144=2^4 x 3^2\n")
cat("\n")
# 
cat("*** RUN ***\n")
ptmtotal <- proc.time()
cat("\n")
F2 <- planor.factors( names=c(LETTERS[1:4], "bloc"), nlevels=c(6,6,4,2,6) )
M2 <- planor.model( model=~bloc+(A+B+C+D)^2, estimate=~A+B+C+D )
ptm <- proc.time()
K2 <- planor.designkey(factors=F2, modlist=M2, nunits=144,
                       base=~A+B+D, max.sol=2)
cat("\n")
ptm <- proc.time()
P2 <- design.listofdesignkeys(key=K2, select=c(1,1))
ptm <- proc.time()
resum <- summary.designkey(pick.listofdesignkeys(K2,c(1,1)))
cat("TEMPS summary(pick)", proc.time()-ptm,"\n")
cat("TEMPS total", proc.time()-ptmtotal,"\n")

